{
	"name": "luky/console",
	"description": "Some fancy utilities around symfony/console.",
	"homepage": "https://github.com/Arziel/php-console",
	"readme": "https://github.com/Arziel/php-console/blob/master/README.md",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lukas \"Arziel\" Klima",
			"email": "arziel12@gmail.com",
			"homepage": "klima.dev"
		}
	],
	"require": {
		"php": ">=7.2",
		"symfony/console": "^3.0 | ^4.0"
	},
	"autoload": {
		"psr-4": {
			"Luky\\Console\\": "src/"
		}
	},
	"require-dev": {
		"mockery/mockery": "^1.0",
		"codeception/codeception": "^2.3",
		"slevomat/coding-standard": "^4.1",
		"phpstan/phpstan": "^0.9.1",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"jakub-onderka/php-console-highlighter": "^0.3.2",
		"tracy/tracy": "^2.4"
	},
	"archive": {
		"exclude": [
			"bin",
			"docs",
			"README.md"
		]
	}
}
